

<div class="row">
  <div class="col-md-4"></div>
  <div class="col-md-4">
    <h2>Assign Projects </h2>
    <%= form_for @history, :url => project_assign_project_histories_path(@history) do |f| %>

      <div class="form-group">
        <%= f.collection_select(:project_id, @projects, :id, :title, {:include_blank => false}, {:class => 'form-control'}) %>
      </div>

      <div class="form-group">
        <%= f.collection_select(:user_id, @developer, :id, :name, {:include_blank => false}, {:class => 'form-control'}) %>
      </div>

      <div class="form-group">
        <%= f.submit %>
      </div>

    <% end %>
  </div>
</div>
